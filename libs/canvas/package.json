{
  "name": "@guitar-chords/canvas",
  "description": "Use Canvas to generate guitar chords or edit it.",
  "authors": "Capricorncd",
  "version": "0.0.10",
  "type": "module",
  "main": "./dist/guitar-chords.umd.js",
  "module": "./dist/guitar-chords.es.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/guitar-chords.es.js",
      "default": "./dist/guitar-chords.umd.js",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "vite build && node ../../scripts/after-build.mjs --dir=dist",
    "pl": "npm run build && npm publish",
    "pl:first": "npm publish --access public"
  },
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "guitar-chords", 
    "guitar", 
    "chords", 
    "svg", 
    "canvas", 
    "Ukulele", 
    "diagram", 
    "javascript", 
    "typescript"
  ],
  "homepage": "https://github.com/capricorncd/guitar-chords#readme",
  "author": "Capricorncd <capricorncd@qq.com>",
  "license": "MIT"
}
